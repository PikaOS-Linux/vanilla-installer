#: vanilla_installer/gtk/dialog-confirm.ui:35
msgid "Install Vanilla OS"
msgstr "A Vanilla OS telepítése"

#: vanilla_installer/gtk/dialog-confirm.ui:6
#: vanilla_installer/gtk/dialog-confirm.ui:24
msgid "Confirm Installation"
msgstr "Telepítés megerősítése"

